{
  "name": "prime-numbers-from-arrays",
  "version": "2.0.0",
  "description": "Get all prime numbers from arrays, sorted ascending",
  "main": "./dist/prime-numbers-from-arrays.min.js",
  "scripts": {
    "rollup": "rollup ./src/prime-numbers-from-arrays.js --file ./dist/prime-numbers-from-arrays.bundle.js --format cjs",
    "babel": "npx babel ./dist/prime-numbers-from-arrays.bundle.js --out-file ./dist/prime-numbers-from-arrays.babel.js --source-map",
    "uglify": "uglifyjs --output ./dist/prime-numbers-from-arrays.min.js --compress --mangle -- ./dist/prime-numbers-from-arrays.babel.js",
    "build": "npm run rollup & npm run babel & npm run uglify",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radoslawkuswik/prime-numbers-from-arrays.git"
  },
  "keywords": [
    "prime numbers",
    "javascript"
  ],
  "author": "Radosław Kuświk <kontakt@radoslawkuswik.pl> (https://radoslawkuswik.pl)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/radoslawkuswik/prime-numbers-from-arrays/issues"
  },
  "homepage": "https://github.com/radoslawkuswik/prime-numbers-from-arrays#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.5.0",
    "rollup": "^0.65.0",
    "uglify-js": "^3.4.8"
  }
}
